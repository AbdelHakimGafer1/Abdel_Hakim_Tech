{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/_components/email-template.jsx", "app/api/send-email/route.js", "app/_components/src/components/nav-desktop.jsx", "app/_components/src/components/nav-mobile.jsx", "app/_components/src/routes.jsx", "app/_components/src/routes.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
